{
  // "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
   "javascript",
   "javascriptreact",
   { "language": "typescript", "autoFix": true },
   { "language": "typescriptreact", "autoFix": true }
],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
